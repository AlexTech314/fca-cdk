    "keywords": "node scripts/find-keywords.js"
  },
  "devDependencies": {
    "@types/jest": "^30",
    "@types/node": "^24.10.1",
    "aws-cdk": "2.1104.0",
    "jest": "^30",
    "jscpd": "^4.0.6",
    "leasot": "^14.4.0",
    "ts-jest": "^29",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.236.0",
    "cdk-fck-nat": "^1.6.22",
    "constructs": "^10.0.0"
  }
